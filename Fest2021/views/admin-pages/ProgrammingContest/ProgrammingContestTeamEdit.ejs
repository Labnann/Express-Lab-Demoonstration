<div id="content">
    <!---------------------------COACH-------------------------->
    <%console.log("Entered edt view")%>

    <h3>Coach Information</h3>
    <div class="row">
        <div class="col">
            <!-- Name input -->
            <div class="form-outline">
                <input type="text" id="name_c" value="<%=team.coach.name%>" onchange="programmingParams.edit.coach.name=this.value" class="form-control"/>
                <label class="form-label" for="name_c">Name</label>
            </div>
        </div>
        <!-- Phone input -->
        <div class="col">
            <div class="form-outline">
                <input type="number" id="phone_c" value="<%=team.coach.phone%>" onchange="programmingParams.edit.coach.phone=this.value" class="form-control"/>
                <label class="form-label" for="phone_c">Phone</label>
            </div>
        </div>
        <!-- Email input -->
        <div class="col">
            <div class="form-outline">
                <input type="email" id="email_c"  value="<%=team.coach.email%>" onchange="programmingParams.edit.coach.email=this.value" class="form-control"/>
                <label class="form-label" for="email_c">Email address</label>
            </div>
        </div>

        <!-- Tshirt input -->
        <div class="col">
            T-Shirt Size
            <div>

                <label class="radio-inline-ts">
                    <input type="radio" name="tsc-radio" onclick="programmingParams.edit.coach.shirt =`L`" <%if(team.coach.shirt===`L`){%> <%="checked"%> <%}%>   > L
                </label>
                <label class="radio-inline-ts">
                    <input type="radio" name="tsc-radio" onclick="programmingParams.edit.coach.shirt =`XL`" <%if(team.coach.shirt===`XL`){%> <%="checked"%> <%}%>  > XL
                </label>
                <label class="radio-inline-ts">
                    <input type="radio" name="tsc-radio" onclick="programmingParams.edit.coach.shirt =`XXL`" <%if(team.coach.shirt===`XXL`){%> <%="checked"%> <%}%>  > XXL
                </label>
            </div>
        </div>
    </div>

    <hr/>
